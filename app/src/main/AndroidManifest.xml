<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dims.animationsdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RVLibraryAnimScreen"></activity>
        <activity android:name=".KeyFrameAnimOnConstraintLayout" />
        <activity android:name=".TransitionNoScenesScreen" />
        <activity android:name=".TransitionsScreen" />
        <activity android:name=".AnimatedVectorDrawableScreen" />
        <activity android:name=".AnimationDrawableScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>